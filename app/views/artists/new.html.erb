<h1>New Artist Form </h1>

<% if flash[:errors]%>
    <% flash[:errors].each do |error|%>
        <%= error %><br>
    <%end%>
<%end%>

<%= form_for @artist do |f|%> 
<label>Name: </label>
<%= f.text_field :name%><br>

<label>Age: </label>
<%= f.text_field :age%><br>

<label>Title: </label>
<%= f.text_field :title %><br>

<%= f.submit %>
<% end %>